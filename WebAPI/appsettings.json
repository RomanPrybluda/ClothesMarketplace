{
  "ConnectionStrings": {
    "Default": "ADD_FROM_SECRETS"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtTokenOptions": {
    "Issuer": "your_issuer",
    "Audience": "your_audience",
    "Key": "YourVeryLongSecureRandomGeneratedKeyWithAtLeast32Characters"
  },
  "Email": {
    "SmtpServer": "smtp.gmail.com",
    "Port": "587",
    "Username": "your-email@gmail.com",
    "Password": "your-email-password",
    "From": "your-email@gmail.com"
  },
  "ImageEncoderSettings": {
    "Formats": [
      {
        "Name": "Jpeg",
        "Extensions": [ ".jpg", ".jpeg" ],
        "Settings": {
          "Quality": 75
        }
      },
      {
        "Name": "Png",
        "Extensions": [ ".png" ],
        "Settings": {
          "CompressionLevel": 9
        }
      },
      {
        "Name": "Webp",
        "Extensions": [ ".webp" ],
        "Settings": {
          "Quality": 80
        }
      }
    ]
  },
  "AllowedHosts": "*"
}